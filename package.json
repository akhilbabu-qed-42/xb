{
  "name": "@drupal/experience_builder",
  "description": "A React application powering the Experience Builder UI.",
  "homepage": "https://www.drupal.org/project/experience_builder",
  "private": true,
  "workspaces": [
    "lib/astro-hydration"
  ],
  "version": "0.0.0",
  "engines": {
    "node": "^20.0.0"
  },
  "type": "module",
  "scripts": {
    "drupaldev": "VITE_DRUPAL=true vite",
    "start": "vite",
    "build": "npm run type-check && vite build && npm run build:astro",
    "build:astro": "npm run build -w lib/astro-hydration",
    "preview": "vite preview",
    "test": "vitest run",
    "prettier:fix": "prettier .. --write --config ../.prettierrc.json --ignore-path ../.prettierignore",
    "prettier:check": "prettier .. --check --config ../.prettierrc.json --ignore-path ../.prettierignore",
    "lint:eslint": "eslint . --max-warnings=0",
    "lint": "npm run lint:eslint",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc --noEmit --pretty",
    "cy:verify": "cypress verify",
    "cy:info": "cypress info",
    "cy:version": "cypress version",
    "cy:cache:list": "cypress cache list",
    "cy:run": "cypress run --browser chrome",
    "cy:run:record": "cypress run --record",
    "cy:open": "cypress open",
    "cy:component": "cypress run --component --browser electron",
    "storybook": "storybook dev --port 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-javascript": "^6.2.2",
    "@cypress/webpack-preprocessor": "^6.0.2",
    "@radix-ui/react-accordion": "^1.2.1",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-menubar": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.1",
    "@radix-ui/themes": "^3.0.4",
    "@reduxjs/toolkit": "^2.0.1",
    "@swc/wasm-web": "^1.10.14",
    "@uiw/codemirror-theme-github": "^4.23.7",
    "@uiw/react-codemirror": "^4.23.7",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "ajv-formats-draft2019": "^1.6.1",
    "babylon": "^6.18.0",
    "clsx": "^2.1.1",
    "cypress-axe": "^1.5.0",
    "cypress-real-events": "^1.13.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "markdown-to-jsx": "^7.7.3",
    "qs": "^6.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-hotkeys-hook": "^4.5.0",
    "react-mosaic-component": "^6.1.1",
    "react-redux": "^9.1.0",
    "react-router": "^6.25.0",
    "react-router-dom": "^6.25.0",
    "redux-undo": "^1.1.0",
    "sortablejs": "^1.15.2",
    "tailwindcss-in-browser": "^0.1.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.2",
    "@eslint/js": "^9.3.0",
    "@storybook/addon-essentials": "^8.4.2",
    "@storybook/addon-interactions": "^8.4.2",
    "@storybook/addon-onboarding": "^8.4.2",
    "@storybook/blocks": "^8.4.2",
    "@storybook/react": "^8.4.2",
    "@storybook/react-vite": "^8.4.2",
    "@storybook/test": "^8.4.2",
    "@testing-library/cypress": "^10.0.2",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/babylon": "^6.16.9",
    "@types/lodash": "^4.17.0",
    "@types/qs": "^6.9.18",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/sortablejs": "^1.15.8",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.2.1",
    "css-loader": "^7.1.2",
    "cypress": "^13.15.1",
    "cypress-terminal-report": "^6.1.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-chai-friendly": "^1.0.1",
    "eslint-plugin-cypress": "^3.6.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-storybook": "^0.11.0",
    "jsdom": "^23.2.0",
    "prettier": "^3.2.1",
    "storybook": "^8.4.2",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.9.0",
    "vite": "^5.0.11",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^1.2.0"
  }
}
